class Hogar {
  const mugre 
  const confort

  method esBueno()=
    mugre <= (confort / 2)
}

class Huerta {
  const capacidadDeProduccion
  const nivel
  
  method esBueno()=
    capacidadDeProduccion > nivel
}

class Mascota {
  const nivelDeSalud

  method esBueno()=
    nivelDeSalud > 250
}



